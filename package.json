{
  "name": "@danbruegge/resume",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:page": "next build && next export",
    "build:pdf": "wkhtmltopdf --quiet --enable-local-file-access --disable-javascript --grayscale --page-size A4 --margin-top 2cm --margin-bottom 2cm out/index.html public/cv.pdf",
    "build": "yarn build:page && yarn build:pdf",
    "deploy": "sh deploy.sh",
    "start": "next start",
    "format": "prettier --ignore-path .gitignore --write '**/*.{js,jsx,ts,tsx,json}'",
    "lint": "next lint",
    "types": "tsc --noEmit"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  },
  "dependencies": {
    "date-fns": "^2.16.1",
    "next": "^11.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.2.1",
    "tailwindcss": "^2.2.17",
    "twin.macro": "^2.8.1"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^1.13.3",
    "babel-plugin-twin": "^1.0.2",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^7.0.4",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "react-is": "^17.0.2",
    "typescript": "^4.1.3"
  }
}
