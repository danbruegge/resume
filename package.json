{
  "name": "@danbruegge/resume",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:page": "next build && next export",
    "build:pdf": "wkhtmltopdf --print-media-type --quiet --enable-local-file-access --disable-javascript --grayscale --page-size A4 --margin-top 2cm --margin-bottom 2cm out/index.html public/cv.pdf",
    "build": "pnpm build:page && pnpm build:pdf",
    "deploy": "sh deploy.sh",
    "start": "next start",
    "format": "prettier --ignore-path .gitignore --write '**/*.{js,jsx,ts,tsx,json}'",
    "lint": "next lint",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "date-fns": "^2.30.0",
    "next": "^13.4.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-config-next": "^13.4.12",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "pretty-quick": "^3.1.3",
    "react-is": "^18.2.0",
    "tailwind-merge": "^1.14.0",
    "typescript": "^5.1.6"
  }
}
