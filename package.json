{
  "name": "@danbruegge/resume",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build:page": "next build",
    "build:pdf": "node scripts/generate-pdf.mjs",
    "build:docx": "node scripts/generate-docx.mjs",
    "build": "pnpm build:page && pnpm build:pdf && pnpm build:docx",
    "deploy": "sh deploy.sh",
    "start": "next start",
    "format": "prettier --ignore-path .gitignore --write '**/*.{js,jsx,ts,tsx,json}'",
    "lint": "next lint",
    "types": "tsc --noEmit",
    "postinstall": "npx puppeteer browsers install chrome"
  },
  "dependencies": {
    "date-fns": "^3.6.0",
    "next": "15.5.4",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tailwindcss": "^3.4.3"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/react": "19.1.17",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.50.0",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "html-to-docx": "^1.8.0",
    "husky": "^9.0.11",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "pretty-quick": "^4.0.0",
    "puppeteer": "^24.23.0",
    "react-is": "19.1.1",
    "tailwind-merge": "^2.3.0",
    "typescript": "^5.4.5"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.1.17"
    }
  }
}
